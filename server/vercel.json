{
    "installCommand": "npm install --legacy-peer-deps",
  {
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "src/index.js",
      "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers":{
      "Access-Control-Allow-Orgin":"*"
    }}
    ]
